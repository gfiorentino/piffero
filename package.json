{
  "name": "piffero",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "tsc && jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "dev": "node --inspect=5858 -r ts-node/register dev/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gfiorentino/piffero.git"
  },
  "keywords": [
    "json",
    "sax",
    "jsonpath",
    "parser",
    "stream",
    "progressive",
    "event",
    "emitter",
    "async"
  ],
  "author": "Giovanni Fiorentino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gfiorentino/piffero/issues"
  },
  "homepage": "https://github.com/gfiorentino/piffero#readme",
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.2",
    "eslint": "^7.12.0",
    "express": "^4.17.1",
    "jasmine": "^3.6.2",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {}
}
