{
  "name": "piffero",
  "version": "1.1.4-beta8",
  "description": "",
  "main": "./dist/index.js",
  "browser": "./dist/b2/browser.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "jasmine-ts JASMINE_CONFIG_PATH=jasmine.json",
    "nyc:report": "nyc report",
    "coverage": "nyc npm run test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build ",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "dev": "node --inspect=5858 -r ts-node/register dev/index.ts",
    "benchmark": "npm run build && cd benchmarking && node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gfiorentino/piffero.git"
  },
  "keywords": [
    "json",
    "sax",
    "jsonpath",
    "parser",
    "stream",
    "progressive",
    "event",
    "emitter",
    "async"
  ],
  "author": "Giovanni Fiorentino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gfiorentino/piffero/issues"
  },
  "homepage": "https://github.com/gfiorentino/piffero#readme",
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jasmine": "^3.6.3",
    "@types/node": "^14.14.22",
    "base64-js": "^1.5.1",
    "benchmark": "^2.1.4",
    "coveralls": "^3.1.0",
    "errors": "^0.3.0",
    "eslint": "^7.19.0",
    "express": "^4.17.1",
    "ieee754": "^1.2.1",
    "istanbul": "^0.4.5",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "ts-loader": "^8.0.14",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "esprima": "^4.0.1",
    "static-eval": "^2.1.0",
    "underscore": "^1.12.0"
  }
