{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node", 
            "request": "launch", 
            "name": "Jasmine Current File", 
            "program": "${workspaceRoot}\\node_modules\\jasmine\\bin\\jasmine.js",
            "args": ["--config=./jasmine.json", "${file}"],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "name": "Express attach",
            "type": "node",
            "request": "attach",
            "port": 5858
        }
    ]
}